{
	"compact": false,
	"sourceMaps": "both",
	"env": {
		"test": {
			"plugins": ["transform-decorators-legacy"],
			"presets": [
				["env", {
					"targets": {
						"node": "current"
					}
				}],
				"stage-1"
			]
		},
		"development": {
			"presets": [
				["env", {
					"targets": {
						"browsers": ["> 1% in US", "last 2 versions", "safari >= 6"]
					},
					"modules": false
				}],
				"stage-1"
			],
			"plugins": [
				"transform-decorators-legacy",
				"external-helpers"
			]
		}
	}
}
