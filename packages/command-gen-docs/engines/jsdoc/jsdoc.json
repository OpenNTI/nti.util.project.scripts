{
	"plugins": ["plugins/markdown"],
	"opts": {
		"destination": "./docs",
		"encoding": "utf8",
		"recurse": true,
		"private": true,
		"package": "package.json"
	},
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)(_|docs|node_modules)"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"useLongnameInNav": true,
		"showInheritedInNav": true
	}
}
