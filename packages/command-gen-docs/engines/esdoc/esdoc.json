{
	"source": "./src",
	"destination": "./docs",
	"includes": ["\\.m?jsx?$"],
	"excludes": ["__test__"],
	"plugins": [
		{"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
		{
			"name": "esdoc-standard-plugin",
			"option": {
				"test": {
					"source": "./src"
				}
			}
		}
	]
}
