{
	"extensions": {
		"recommendations": [
			"aaron-bond.better-comments",
			"adamhartford.vscode-base64",
			"adpyke.codesnap",
			"artdiniz.quitcontrol-vscode",
			"atlassian.atlascode",
			"britesnow.vscode-toggle-quotes",
			"christian-kohler.path-intellisense",
			"davidanson.vscode-markdownlint",
			"dbaeumer.vscode-eslint",
			"eamodio.gitlens",
			"editorconfig.editorconfig",
			"esbenp.prettier-vscode",
			"euskadi31.json-pretty-printer",
			"formulahendry.auto-rename-tag",
			"github.vscode-pull-request-github",
			"jakewilson.vscode-placeholder-images",
			"jpoissonnier.vscode-styled-components",
			"mhutchie.git-graph",
			"mikestead.dotenv",
			"ms-vscode-remote.vscode-remote-extensionpack",
			"ms-vsliveshare.vsliveshare-pack",
			"sleistner.vscode-fileutils",
			"streetsidesoftware.code-spell-checker",
			"stylelint.vscode-stylelint",
			"usernamehw.errorlens",
			"visualstudioexptteam.vscodeintellicode",
			"wmaurer.change-case"
		]
	},
	"launch": {
		"configurations": [
			{
				"type": "node",
				"request": "launch",
				"name": "Debug Jest on Current File",
				"cwd": "${fileWorkspaceFolder}",
				"program": "${fileWorkspaceFolder}../../../node_modules/.bin/jest",
				"autoAttachChildProcesses": true,
				"sourceMaps": true,
				"args": [
					"--runInBand",
					"--no-cache",
					"${fileBasenameNoExtension}"
				]
			}
		],
		"compounds": []
	},
	"settings": {
		"cSpell.words": ["Savepoints", "Transcripted", "icomoon"],
		"css.validate": false,
		"editor.codeActionsOnSave": {
			"source.fixAll": true,
			"source.fixAll.stylelint": false
		},
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.linkedEditing": true,
		"editor.renderControlCharacters": true,
		"editor.suggest.showStatusBar": true,
		"errorLens.enabledDiagnosticLevels": ["error", "warning"],
		"eslint.codeActionsOnSave.mode": "problems",
		"eslint.lintTask.enable": true,
		"files.exclude": {
			"**/node_modules/**": true
		},
		"git.autofetch": true,
		"git.autoStash": true,
		"git.pruneOnFetch": true,
		"git.pullTags": false,
		"git.rebaseWhenSync": true,
		"javascript.updateImportsOnFileMove.enabled": "always",
		"less.validate": false,
		"scm.alwaysShowRepositories": false,
		"scm.repositories.visible": 0,
		"scss.validate": false,
		"stylelint.reportInvalidScopeDisables": true,
		"stylelint.reportNeedlessDisables": true,
		"prettier.requireConfig": true
	}
}
