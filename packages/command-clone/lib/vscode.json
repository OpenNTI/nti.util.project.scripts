{
	"extensions": {
		"recommendations": [
			"aaron-bond.better-comments",
			"adamhartford.vscode-base64",
			"adpyke.codesnap",
			"artdiniz.quitcontrol-vscode",
			"atlassian.atlascode",
			"britesnow.vscode-toggle-quotes",
			"christian-kohler.path-intellisense",
			"davidanson.vscode-markdownlint",
			"dbaeumer.vscode-eslint",
			"eamodio.gitlens",
			"editorconfig.editorconfig",
			"esbenp.prettier-vscode",
			"euskadi31.json-pretty-printer",
			"formulahendry.auto-rename-tag",
			"github.vscode-pull-request-github",
			"jakewilson.vscode-placeholder-images",
			"jpoissonnier.vscode-styled-components",
			"mhutchie.git-graph",
			"mikestead.dotenv",
			"ms-vscode-remote.vscode-remote-extensionpack",
			"ms-vsliveshare.vsliveshare-pack",
			"sleistner.vscode-fileutils",
			"streetsidesoftware.code-spell-checker",
			"stylelint.vscode-stylelint",
			"usernamehw.errorlens",
			"visualstudioexptteam.vscodeintellicode",
			"wmaurer.change-case"
		]
	},
	"launch": {
		"compounds": [],
		"configurations": [
			{
				"args": [
					"--runInBand",
					"--no-cache",
					"${fileBasenameNoExtension}"
				],
				"autoAttachChildProcesses": true,
				"cwd": "${fileWorkspaceFolder}",
				"name": "Debug Jest on Current File",
				"program": "${fileWorkspaceFolder}../../../node_modules/.bin/jest",
				"request": "launch",
				"sourceMaps": true,
				"type": "node"
			}
		]
	},
	"settings": {
		"cSpell.words": [
			"Savepoints",
			"Transcripted",
			"autofetch",
			"gitlens",
			"icomoon"
		],
		"css.validate": false,
		"editor.codeActionsOnSave": {
			"source.fixAll": true,
			"source.fixAll.stylelint": false,
			"source.fixAll.ts": false
		},
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"errorLens.enabledDiagnosticLevels": ["error", "warning"],
		"emmet.excludeLanguages": [
			"javascript",
			"javascriptreact",
			"css",
			"markdown",
			"typescript",
			"typescriptreact"
		],
		"eslint.codeActionsOnSave.mode": "problems",
		"eslint.lintTask.enable": true,
		"eslint.validate": [
			"javascript",
			"javascriptreact",
			"typescript",
			"typescriptreact"
		],
		"files.exclude": {
			"**/node_modules/**": true,
			"**/dist/**": true
		},
		"git.autoStash": true,
		"git.autofetch": true,
		"git.enableSmartCommit": true,
		"git.pruneOnFetch": true,
		"git.pullTags": false,
		"git.rebaseWhenSync": true,
		"gitlens.advanced.fileHistoryFollowsRenames": true,
		"gitlens.advanced.messages": {
			"suppressLineUncommittedWarning": true
		},
		"javascript.updateImportsOnFileMove.enabled": "always",
		"less.validate": false,
		"npm-intellisense.packageSubfoldersIntellisense": true,
		"npm.packageManager": "npm",
		"npm.runSilent": true,
		"npm.validate.enable": false,
		"prettier.requireConfig": true,
		"scm.alwaysShowRepositories": false,
		"scm.repositories.visible": 0,
		"scss.validate": false,
		"search.useGlobalIgnoreFiles": true,
		"stylelint.reportInvalidScopeDisables": true,
		"stylelint.reportNeedlessDisables": true,
		"typescript.enablePromptUseWorkspaceTsdk": true,
		"typescript.tsdk": "node_modules/typescript/lib/"
	}
}
